{
  "name": "supr/supr",
  "description": "supr integration extension",
  "type": "typo3-cms-extension",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "TYPO3 GmbH",
      "role": "Developer",
      "homepage": "https://typo3.com/"
    },
    {
      "name": "Wirecard Technologies GmbH",
      "role": "Developer",
      "homepage": "https://www.supr.com/"
    }
  ],
  "replace": {
    "supr/supr": "self.version"
  },
  "require": {
    "typo3/cms-core": "^8.7",
    "typo3/cms-fluid-styled-content": "^8.7"
  },
  "require-dev": {
    "typo3/testing-framework": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Supr\\Supr\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Supr\\Supr\\Tests\\": "Tests/"
    }
  }
}
